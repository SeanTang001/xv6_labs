set confirm off
set architecture riscv:rv64
target remote 127.0.0.1:1234
symbol-file kernel/kernel
set disassemble-next-line auto
set riscv use-compressed-breakpoints yes
tui new-layout example {-horizontal regs 1 asm 1 src 1 } 1 status 1 cmd 1
layout example
tui reg general
file user/_uthread
watch all_thread[1]->state
watch all_thread[2]->state
watch all_thread[3]->state
